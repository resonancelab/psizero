  "types": {
    "IChingRequest": {
      "type": "object",
      "description": "Request for I-Ching oracle consultation",
      "properties": {
        "question": {
          "type": "string",
          "description": "The question to ask the oracle",
          "minLength": 1,
          "maxLength": 500,
          "example": "What direction should I take in my career?"
        },
        "context": {
          "type": "string",
          "description": "Context or domain for the question",
          "default": "general",
          "enum": ["general", "career", "relationships", "health", "spiritual", "financial"],
          "example": "career"
        },
        "querent": {
          "type": "string",
          "description": "Identifier for the person asking",
          "default": "seeker",
          "example": "seeker"
        },
        "steps": {
          "type": "integer",
          "description": "Number of evolution steps for the consultation",
          "default": 7,
          "minimum": 3,
          "maximum": 21,
          "example": 7
        },
        "config": {
          "$ref": "#/types/IChingConfig",
          "description": "Optional configuration parameters"
        }
      },
      "required": ["question"]
    },
    "IChingConfig": {
      "type": "object",
      "description": "Configuration for I-Ching consultation",
      "properties": {
        "oracle_mode": {
          "type": "string",
          "description": "Oracle consultation mode",
          "default": "quantum",
          "enum": ["quantum", "classical", "hybrid"]
        },
        "divination_method": {
          "type": "string",
          "description": "Method for hexagram generation",
          "default": "quantum_collapse",
          "enum": ["quantum_collapse", "entropy_dynamics", "resonance_pattern"]
        },
        "wisdom_accumulation": {
          "type": "boolean",
          "description": "Enable wisdom accumulation across consultations",
          "default": true
        },
        "pattern_recognition": {
          "type": "boolean",
          "description": "Enable pattern recognition in sequences",
          "default": true
        },
        "cosmic_alignment": {
          "type": "boolean",
          "description": "Consider cosmic alignment factors",
          "default": true
        },
        "temporal_prediction": {
          "type": "boolean",
          "description": "Enable temporal prediction elements",
          "default": false
        },
        "max_readings_per_session": {
          "type": "integer",
          "description": "Maximum readings per session",
          "default": 3,
          "minimum": 1,
          "maximum": 10
        },
        "reading_validity_hours": {
          "type": "integer",
          "description": "Hours the reading remains valid",
          "default": 24,
          "minimum": 1,
          "maximum": 168
        },
        "evolution_steps": {
          "type": "integer",
          "description": "Number of evolution steps",
          "default": 10,
          "minimum": 3,
          "maximum": 21
        },
        "resonance_threshold": {
          "type": "number",
          "description": "Threshold for resonance detection",
          "default": 0.7,
          "minimum": 0.1,
          "maximum": 1.0
        },
        "certainty_threshold": {
          "type": "number",
          "description": "Threshold for certainty in results",
          "default": 0.8,
          "minimum": 0.1,
          "maximum": 1.0
        },
        "timeout_seconds": {
          "type": "integer",
          "description": "Maximum consultation time",
          "default": 60,
          "minimum": 10,
          "maximum": 300
        }
      }
    },
    "IChingResponse": {
      "type": "object",
      "description": "Response from I-Ching oracle consultation",
      "properties": {
        "question": {
          "type": "string",
          "description": "The original question"
        },
        "context": {
          "type": "string",
          "description": "Context of the consultation"
        },
        "querent": {
          "type": "string",
          "description": "The querent identifier"
        },
        "result": {
          "$ref": "#/types/DivinationResult",
          "description": "Main divination result from engine"
        },
        "telemetry": {
          "type": "array",
          "items": {"$ref": "#/types/TelemetryPoint"},
          "description": "Evolution telemetry data"
        },
        "sequence": {
          "type": "array",
          "items": {"$ref": "#/types/HexagramStep"},
          "description": "Hexagram evolution sequence"
        },
        "final_hexagram": {
          "$ref": "#/types/Hexagram",
          "description": "Final stabilized hexagram"
        },
        "interpretation": {
          "$ref": "#/types/Interpretation",
          "description": "Oracle interpretation and guidance"
        },
        "stabilized": {
          "type": "boolean",
          "description": "Whether the consultation stabilized"
        },
        "metrics": {
          "$ref": "#/types/IChingMetrics",
          "description": "Consultation metrics"
        },
        "timing": {
          "$ref": "#/types/TimingInfo",
          "description": "Timing information"
        }
      },
      "required": ["question", "sequence", "final_hexagram", "stabilized", "metrics"]
    },
    "DivinationResult": {
      "type": "object",
      "description": "Core divination result from I-Ching engine",
      "properties": {
        "primary_hexagram": {
          "$ref": "#/types/EngineHexagram",
          "description": "Primary hexagram from consultation"
        },
        "certainty": {
          "type": "number",
          "description": "Certainty level of the result"
        },
        "cosmic_alignment": {
          "type": "number",
          "description": "Cosmic alignment factor"
        },
        "interpretation": {
          "$ref": "#/types/EngineInterpretation",
          "description": "Engine-generated interpretation"
        }
      }
    },
    "EngineHexagram": {
      "type": "object",
      "description": "Hexagram as returned by I-Ching engine",
      "properties": {
        "number": {
          "type": "integer",
          "description": "Hexagram number (1-64)"
        },
        "name": {
          "type": "string",
          "description": "Hexagram name"
        },
        "meaning": {
          "type": "object",
          "properties": {
            "core_meaning": {
              "type": "string",
              "description": "Core meaning of the hexagram"
            }
          }
        },
        "lines": {
          "type": "array",
          "items": {"$ref": "#/types/LineType"},
          "description": "Array of 6 line types"
        },
        "trigrams": {
          "type": "array",
          "items": {"$ref": "#/types/EngineTrigramType"},
          "minItems": 2,
          "maxItems": 2,
          "description": "Lower and upper trigrams"
        }
      }
    },
    "LineType": {
      "type": "object",
      "description": "I-Ching line type",
      "properties": {
        "is_yang": {
          "type": "boolean",
          "description": "Whether the line is yang (true) or yin (false)"
        },
        "is_changing": {
          "type": "boolean",
          "description": "Whether the line is changing"
        }
      }
    },
    "EngineTrigramType": {
      "type": "object",
      "description": "Trigram as returned by engine",
      "properties": {
        "name": {
          "type": "string",
          "description": "Trigram name"
        },
        "element": {
          "type": "string",
          "description": "Associated element"
        },
        "attribute": {
          "type": "string",
          "description": "Trigram attribute/quality"
        },
        "lines": {
          "type": "array",
          "items": {"$ref": "#/types/LineType"},
          "minItems": 3,
          "maxItems": 3,
          "description": "Array of 3 line types"
        }
      }
    },
    "EngineInterpretation": {
      "type": "object",
      "description": "Interpretation as returned by engine",
      "properties": {
        "situation": {
          "type": "string",
          "description": "Current situation analysis"
        },
        "guidance": {
          "type": "string",
          "description": "Guidance and advice"
        },
        "action": {
          "type": "string",
          "description": "Recommended action"
        },
        "context": {
          "type": "string",
          "description": "Contextual information"
        },
        "confidence": {
          "type": "number",
          "description": "Confidence in the interpretation"
        }
      }
    },
    "HexagramStep": {
      "type": "object",
      "description": "Single step in hexagram evolution",
      "properties": {
        "step": {
          "type": "integer",
          "description": "Step number in sequence"
        },
        "hexagram": {
          "type": "string",
          "description": "Hexagram pattern (binary string)"
        },
        "binary": {
          "type": "string",
          "description": "Binary representation"
        },
        "name": {
          "type": "string",
          "description": "Hexagram name"
        },
        "entropy": {
          "type": "number",
          "description": "Entropy level at this step"
        },
        "attractorProximity": {
          "type": "number",
          "description": "Proximity to attractor state"
        },
        "timestamp": {
          "type": "string",
          "format": "date-time",
          "description": "Timestamp of this step"
        }
      },
      "required": ["step", "hexagram", "binary", "entropy", "attractorProximity", "timestamp"]
    },
    "Hexagram": {
      "type": "object", 
      "description": "Complete hexagram information",
      "properties": {
        "pattern": {
          "type": "string",
          "description": "Hexagram pattern"
        },
        "binary": {
          "type": "string",
          "description": "Binary representation"
        },
        "number": {
          "type": "integer",
          "description": "Hexagram number (1-64)"
        },
        "name": {
          "type": "string",
          "description": "Hexagram name"
        },
        "description": {
          "type": "string",
          "description": "Hexagram description"
        },
        "lines": {
          "type": "array",
          "items": {"type": "string"},
          "description": "Line interpretations"
        },
        "trigrams": {
          "$ref": "#/types/Trigrams",
          "description": "Upper and lower trigrams"
        }
      },
      "required": ["pattern", "binary", "number", "name"]
    },
    "Trigrams": {
      "type": "object",
      "description": "Upper and lower trigrams of a hexagram",
      "properties": {
        "upper": {
          "$ref": "#/types/Trigram",
          "description": "Upper trigram"
        },
        "lower": {
          "$ref": "#/types/Trigram",
          "description": "Lower trigram"
        }
      }
    },
    "Trigram": {
      "type": "object",
      "description": "Individual trigram information",
      "properties": {
        "pattern": {
          "type": "string",
          "description": "Trigram pattern (3-bit binary)"
        },
        "name": {
          "type": "string",
          "description": "Trigram name"
        },
        "element": {
          "type": "string",
          "description": "Associated element"
        },
        "quality": {
          "type": "string",
          "description": "Trigram quality/attribute"
        }
      },
      "required": ["pattern", "name", "element", "quality"]
    },
    "Interpretation": {
      "type": "object",
      "description": "Oracle interpretation and guidance",
      "properties": {
        "summary": {
          "type": "string",
          "description": "Summary of the reading"
        },
        "guidance": {
          "type": "string",
          "description": "Guidance and advice"
        },
        "themes": {
          "type": "array",
          "items": {"type": "string"},

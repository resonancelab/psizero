        },
        "symbolism": {
          "type": "object",
          "additionalProperties": {"type": "string"},
          "description": "Symbolic meanings"
        },
        "context": {
          "type": "object",
          "additionalProperties": true,
          "description": "Contextual information"
        },
        "confidence": {
          "type": "number",
          "description": "Confidence in interpretation"
        }
      },
      "required": ["summary", "guidance", "confidence"]
    },
    "IChingMetrics": {
      "type": "object",
      "description": "I-Ching consultation metrics",
      "properties": {
        "finalEntropy": {
          "type": "number",
          "description": "Final entropy level"
        },
        "convergenceRate": {
          "type": "number",
          "description": "Rate of convergence"
        },
        "stabilizationPoint": {
          "type": "integer",
          "description": "Step where stabilization occurred"
        },
        "attractorStrength": {
          "type": "number",
          "description": "Strength of the attractor"
        },
        "symbolicResonance": {
          "type": "number",
          "description": "Symbolic resonance measure"
        }
      }
    },
    "HexagramDatabase": {
      "type": "object",
      "description": "Information about hexagram database",
      "properties": {
